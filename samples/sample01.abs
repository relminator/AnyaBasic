start
      for( var y = 0 to 48 step 1 )
      start
          var dy = 25 - y
          for( var x = 0 to 78 step 1)
          start
              var dx = 40 - x
              var col = sqrt(dx*dx + dy*dy)
              if ( ((col * 3) % 2) == 0 ) then
                  write("#")
              else
                  write(" ")
          end
          print("")
      end
  end
